{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize",
    "node_modules/jsdoc-mermaid",
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category"
  ],
  "typescript": {
    "moduleRoot": "src"
  },
  "package": "package.json",
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src"
    ],
    "exclude": [
      "cli.(ts|js)"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "default": {
      "useLongnameInNav": false
    },
    "cleverLinks": true,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "search": true
  },
  "opts": {
    "encoding": "utf-8",
    "useLongnameInNav": false,
    "template": "./node_modules/better-docs",
    "destination": "./docs/",
    "readme": "./README.md",
    "recurse": true,
    "verbose": true,
    "tutorials": "workdocs/tutorials"
  }
}
